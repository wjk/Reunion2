<Project>
  <Target Name="_CheckForUnsupportedTargetFramework" />
  <Target Name="GetReferenceAssemblyPaths" />
  <Target Name="ResolvePackageAssets" />

  <PropertyGroup>
    <TargetName>$(OutputName)$(TargetExt)</TargetName>
  </PropertyGroup>

  <ImportGroup Condition="'$(WixTargetsPath)' != ''">
    <Import Project="$(WixTargetsPath)" />
    <Import Project="..\signing\CodeSigning.Wix.targets" />
  </ImportGroup>

  <Target Name="_FixBundleEnginePath" BeforeTargets="InscribeBundleEngine">
    <ItemGroup>
      <SignTargetPath Remove="@(SignTargetPath)" />
      <SignTargetPath Include="$(TargetDir)$(TargetName)$(TargetExt)" />
    </ItemGroup>
  </Target>
</Project>
